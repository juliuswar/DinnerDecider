<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorAccent"
        tools:context=".MainActivity">
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/dd_logo" android:id="@+id/imageView3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:contentDescription="TODO"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.11"/>
    <TextView
            android:text="Test"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:contentDescription="TODO"
            android:layout_marginTop="216dp" android:textSize="24sp" app:layout_constraintHorizontal_bias="0.498"
            android:textColor="@android:color/black"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:contentDescription="TODO"
            android:id="@+id/textinput1"
            android:hint="Add new food!" android:layout_marginTop="368dp"/>
    <Button
            android:text="Add"
            android:layout_width="218dp"
            android:layout_height="43dp" android:id="@+id/buttonadd"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:contentDescription="TODO" android:layout_marginTop="432dp"
            app:layout_constraintHorizontal_bias="0.487" android:textColorLink="#009E2828"
            android:background="#5a7e8c"/>
    <Button
            android:text="Decide"
            android:layout_width="411dp"
            android:layout_height="65dp" android:id="@+id/buttondecide"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:contentDescription="TODO"
            android:background="#5a7e8c" android:layout_marginTop="536dp" app:layout_constraintHorizontal_bias="0.0"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:contentDescription="TODO"
            android:id="@+id/button" android:layout_marginTop="304dp" app:layout_constraintHorizontal_bias="0.498"/>

</androidx.constraintlayout.widget.ConstraintLayout>